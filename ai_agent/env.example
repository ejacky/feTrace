# AI Agent 服务环境变量配置

# 服务端口和主机
AI_AGENT_PORT=8002
AI_AGENT_HOST=0.0.0.0
AI_AGENT_DEBUG=False

# DeepSeek API 配置
DEEPSEEK_API_KEY=your-api-key-here
DEEPSEEK_CONNECT_TIMEOUT=15
DEEPSEEK_READ_TIMEOUT=40
DEEPSEEK_RETRY_TOTAL=2
DEEPSEEK_RETRY_CONNECT=2
DEEPSEEK_RETRY_READ=2
DEEPSEEK_RETRY_STATUS=2
DEEPSEEK_BACKOFF_FACTOR=0.6

# 地理编码配置
GEOCODE_ENABLED=True
GEOCODE_MAX_CALLS=3

# 日志级别
LOG_LEVEL=INFO

# Flask 配置
FLASK_ENV=production
FLASK_APP=ai_service.py